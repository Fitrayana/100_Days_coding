public class variabel_umum_java {
    public static void main(String[] args) {
        // Variabel umum
        int umur = 20; //bilangan bulat (integer)
        double  Tinggi = 157.5; //bilangan pecahan (floating-point)
        String nama = "FITRAYANA"; //teks
        boolean Cantik = false; //hanya berisi true or false
        char jk = 'P';

        System.out.println("Nama: " + nama);
        System.out.println("Umur: " + umur + " tahun");
        System.out.println("Tinggi Badan: " + Tinggi + " Cm");
        System.out.println("Apakah Saya Cantik: " + Cantik);
        System.out.println("Jenis Kelamin: " + jk);
    }
}


    
